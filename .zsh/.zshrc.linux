# ------------------------------------------------------------------------------
# autojump
# ------------------------------------------------------------------------------
which brew >/dev/null 2>&1
if [[ $? -eq 0 ]]; then
    [[ -s `brew --prefix`/etc/autojump.sh ]] && source `brew --prefix`/etc/autojump.sh
else
    [[ -s /usr/share/autojump/autojump.sh ]] && source /usr/share/autojump/autojump.sh
fi

# ------------------------------------------------------------------------------
# Aliases
# ------------------------------------------------------------------------------
alias ls='ls -F --color=auto'
