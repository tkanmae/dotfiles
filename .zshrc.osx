# ------------------------------------------------------------------------------
# zsh-notify
# ------------------------------------------------------------------------------
source ~/.zsh/zsh-notify/notify.plugin.zsh


# ------------------------------------------------------------------------------
# autojump
# ------------------------------------------------------------------------------
[[ -s `brew --prefix`/etc/autojump.sh ]] && . `brew --prefix`/etc/autojump.sh


# ------------------------------------------------------------------------------
# Aliases
# ------------------------------------------------------------------------------
alias ls="ls -GF"
alias julia="exec '/Applications/Julia-0.4.0.app/Contents/Resources/julia/bin/julia'"
